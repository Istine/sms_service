{
    "compilerOptions": {
      "target": "es5",                          
      "module": "commonjs",                    
      "lib": ["es6"],                     
      "allowJs": true,
      "outDir": "build",                          
      "rootDir": "src",
      "moduleResolution": "Node",
      "strict": true,         
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*.ts", "__tests__/**/*.ts"],
  "exclude": ["node_modules","rest_api_calls"]
  }